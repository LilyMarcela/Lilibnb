<div class="container no-gutter">

  <% @places.each do |place| %>
    <li>
      <div class="col-sm-3">
        <div class="row">
          <div><img src="<%= place.photo %>"></div>
          <div><h5><%= place.description %></h5></div>
          <div><h6>$<%= place.price %> per night</h6></div>
          <br>
          <div class="type"><%= place.type.name %></div>
          
    
           <% end %>
        </div>
     </div>
    </li>
</div>
